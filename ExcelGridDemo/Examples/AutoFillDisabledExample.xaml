<Window x:Class="ExcelGridDemo.AutoFillDisabledExample"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:p="http://schemas.galador.net/xaml/spreadsheet"
        xmlns:dataGridDemo="clr-namespace:ExcelGridDemo"
        Title="Auto fill disabled" Height="450" Width="900">
    <Window.DataContext>
        <dataGridDemo:ExampleViewModel/>
    </Window.DataContext>
    <Grid>
        <p:DataGrid ItemsSource="{Binding ItemsSource}" IsAutoFillEnabled="False"/>
    </Grid>
</Window>
